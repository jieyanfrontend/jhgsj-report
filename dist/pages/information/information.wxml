<!--pages/information/information.wxml-->

    <view class="weui-cells__title">请正确填写以下全部信息</view>
        <view class="weui-cells weui-cells_after-title">
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">企业名称：</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" focus='true' placeholder="请输入企业名称"/>
                </view>
            </view>
            <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">注册号：</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入注册号"/>
                </view>
            </view>


             <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">负责人：</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入负责人"/>
                </view>
            </view>
             <view class="weui-cell weui-cell_input">
                <view class="weui-cell__hd">
                    <view class="weui-label">地址：</view>
                </view>
                <view class="weui-cell__bd">
                    <textarea maxlength='140'><input class="weui-input" placeholder="请输入地址"/></textarea>
                </view>
            </view>





            <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">手机号:</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入手机号" />
                </view>
                <view class="weui-cell__ft">
                    <view class="weui-vcode-btn">获取验证码</view>
                </view>
            </view>

             <view class="weui-cell weui-cell_input weui-cell_vcode">
                <view class="weui-cell__hd">
                    <view class="weui-label">验证码:</view>
                </view>
                <view class="weui-cell__bd">
                    <input class="weui-input" placeholder="请输入验证码" />
                </view>

                <view class="weui-cell__ft">
                    <image class="weui-vcode-img" src="../images/vcode.jpg" style="width: 108px"></image>
                </view>
            </view>






       


            

        
        
        
  </view>



  <view><button>下一步</button></view>


