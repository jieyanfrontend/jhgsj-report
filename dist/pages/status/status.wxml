<!--pages/status/status.wxml-->

<view class="page">
    
    <view class="page__bd">
       

        <view class="weui-cells__title">查看各企业状态</view>
         <view wx:for="{{statusList}}" wx:key="orgID" >
        <view class="weui-cells weui-cells_after-title">
        
          <view class="weui-cell weui-cell_access">
            

                <view class="weui-cell__hd">
                    <image src="../../images/企业logo.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">
                 <view class="weui-cell__bd">认证状态：{{item.status}}</view>
                 <view class="weui-cell__bd com-wd" >企业名称:{{item.orgName}}</view>
                 <view class="weui-cell__bd com-tm">提交时间：{{item.submitTime}}</view>
                
                </view>
                <view class="weui-cell__ft weui-cell__ft_in-access "><navigator url='' style='color:blue'>点击查看</navigator></view>
          </view>
         </view> 
                 
        </view>
    </view>
</view>
